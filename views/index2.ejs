<!--============from the Journal=========================-->
<section style="margin: 5rem 0rem;">
  <h5 class="text-center mb-4 mt-sm-3">More from the Journal</h5>
<div class="row">
	<%for(var i=2; i<4 ; i++){%>
    <div class="col-md-6">
        <div class="card mb-3">
			<div style="overflow:hidden;">
				<img src="<%=blogPosts[Count -i].blogPhoto%>" class="card-img card-img-top" alt="..."></div>
		  <div class="card-body">
			<h4 style="font-weight : bolder;" class="card-title"><%=blogPosts[Count -i].title%></h4>
			<p class="card-text"><%=blogPosts[Count -i].postSummary%></p>
		  <p class="card-text"><small class="text-muted"><i class="far fa-calendar-alt"></i>&nbsp;<%=blogPosts[Count-i].createdAt%></small></p>
        <a href="blog/<%=blogPosts[Count-i].id%>" class="btn">Continue Reading&nbsp;<i class="fas fa-chevron-right"></i></a>
  			</div>
		</div>
    </div>
    <%}%>
</div>
</section>

